<Window x:Class="PathFinding.GraphSelectionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PathFinding"
        mc:Ignorable="d"
        Title="GraphSelection" Height="450" Width="800">
    
    <StackPanel Orientation="Vertical">

        <Button Height="210" 
                Name="weightedGraphButton"
                Content="Weighted Graph"
                Click="weightedGraphButton_Click"/>
        
        <Button Height="210" 
                Content="Unweighted Graph"
                Name="unwieghtedGraphButton"
                Click="unwieghtedGraphButton_Click"/>

    </StackPanel>
</Window>
